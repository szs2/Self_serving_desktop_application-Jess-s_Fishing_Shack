<AnchorPane prefHeight="558.0" prefWidth="804.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Text fx:id="txtCusLog" layoutX="246.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Login">
         <font>
            <Font name="System Bold" size="40.0" />
         </font></Text>
      <Text layoutX="48.0" layoutY="123.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Name">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Text>
      <Text fx:id="txtCusPassword" layoutX="48.0" layoutY="190.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Text>
      <TextField fx:id="txtfCusUserN" layoutX="193.0" layoutY="97.0" prefHeight="31.0" prefWidth="548.0">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="txtfCusPassword" layoutX="193.0" layoutY="166.0" prefHeight="11.0" prefWidth="548.0">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="btnCusCreate" layoutX="26.0" layoutY="454.0" mnemonicParsing="false" onAction="#createNew" text="Create New Account">
